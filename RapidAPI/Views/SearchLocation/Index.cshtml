@model List<SearchLocationViewModel>
@{
    ViewData["Title"] = "Index";
}

<form method="post">
    <input type="text" name="cityName" placeholder="Şehir İsmi" />
    <button class="btn btn-dark">Ara</button>

</form>



<table class="table table-hover">
    <tr>
        <th>Lokasyon ID</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <th>@item.dest_id</th>
        </tr>
    }
</table>


<h3>@ViewBag.id</h3>